rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Allow read/write access to all documents for development
    // WARNING: This allows anyone to read/write your entire database
    // Only use this for development/testing. Secure your rules for production!
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
